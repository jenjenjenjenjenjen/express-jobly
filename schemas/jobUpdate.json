{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "job"
            ]
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                100
            ]
        },
        "equity": {
            "type": "string",
            "default": "",
            "title": "The equity Schema",
            "examples": [
                "0.5"
            ]
        }
    },
    "examples": [{
        "title": "job",
        "salary": 100,
        "equity": "0.5"
    }]
}